# Environment variables
DEVELOPMENT=true
VALKEY_USERNAME=default
VALKEY_TOKEN=mysecretpassword
VALKEY_HOST=localhost
VALKEY_PORT=14102
LOG_LEVEL=debug
LOG_JSON=false
LOG_COLORIZE_LINE=true
LOG_UNESCAPE_MULTILINE=true
LOG_DISABLE_TIMESTAMP=false

# GoDNS Application ports (14000-14005)
DNS_SERVER_PORT=:53
DNS_SERVER_LIVENESS_PROBE_PORT=:14000
DNS_SERVER_READYNESS_PROBE_PORT=:14001
DNS_ENABLE_HTTP_API=true

# Note: 14002 reserved for future use
HTTP_API_PORT=:14003
HTTP_API_LIVENESS_PROBE_PORT=:14004
HTTP_API_READINESS_PROBE_PORT=:14005

# Docker Compose Infrastructure ports (14100+)
# PostgreSQL: 14100
# Keycloak: 14101
# Valkey: 14102
POSTGRES_PORT=14100
KEYCLOAK_PORT=14101

# Keycloak Configuration
KEYCLOAK_ADMIN_USER=admin
KEYCLOAK_ADMIN_PASSWORD=admin
KEYCLOAK_HOSTNAME=localhost
KEYCLOAK_PROXY=edge
KEYCLOAK_DB_NAME=keycloak
KEYCLOAK_DB_USER=keycloak
KEYCLOAK_DB_PASSWORD=keycloak_password